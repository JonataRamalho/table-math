{
  "expo": {
    "name": "Table Math",
    "slug": "tablemath",
    "version": "1.0.0",
    "orientation": "portrait",
    "description": "Tabuada simples que ajuda facilmente a praticar as operações de adição, subtração, multiplicação e divisão.",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "androidNavigationBar": {
      "barStyle": "light-content",
      "backgroundColor": "#85AEE2"
    }
  }
}
